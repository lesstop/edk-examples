<wex-definition>
    <meta>
        <group-id>${groupId}</group-id>      
        <artifact-id>${artifactId}</artifact-id>      
        <display-name>Example - Tree</display-name>
        <system>false</system> 
        <description>Extension tree example</description>  
    </meta>
    <build>
        <version>
            <major>${major}</major>
            <minor>${minor}</minor>
            <beta>${beta}</beta>
        </version>
        <host>
            <release>${wex.wt}</release>
        </host>
    </build> 
    
    <install>
        <menus>
            <extension menu="header actions" file="navigation-actionModels" action="edkExampleTree"/>
            
        </menus>
        <actions>
            <page name="edkExampleTree"   url="edkExampleTree.jsp"/>
           
        </actions>
        
    </install>
</wex-definition>