<settings>
    <servers>
        <server>
            <id>sign.wex-cloud.net</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-private-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-releases-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
         <server>
            <id>WINCOM-PRIVATE-REPO</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>WINCOM-RELEASES-REPO</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-windchill-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-snapshots-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-legacy-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-geavio-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
        <server>
            <id>wincom-geavio-snapshots-repo</id>
            <username>[userid]</username>
            <password>[password]</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>default</id>
            <repositories>
                <repository>
                    <id>wincom-private-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/private/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>wincom-releases-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/releases/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>WINCOM-PRIVATE-REPO</id>
                    <url>https://nexus.wex-cloud.net/repository/private/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>WINCOM-RELEASES-REPO</id>
                    <url>https://nexus.wex-cloud.net/repository/releases/</url>
                    <layout>default</layout>
                </repository>

                <repository>
                    <id>wincom-windchill-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/windchill/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>wincom-snapshots-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/snapshots/</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>wincom-legacy-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/legacy/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>wincom-geavio-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/clients-geavio/</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <id>wincom-geavio-snapshots-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/clients-geavio-snapshots/</url>
                    <layout>default</layout>
                </repository>


            </repositories>

            <pluginRepositories>
                <pluginRepository>
                    <id>wincom-private-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/private/</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>wincom-snapshots-repo</id>
                    <url>https://nexus.wex-cloud.net/repository/snapshots/</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>


        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>default</activeProfile>
    </activeProfiles>
</settings>